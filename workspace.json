{
  "version": 2,
  "projects": {
    "electron-groopi-app": {
      "root": "apps/electron-groopi-app",
      "sourceRoot": "apps/electron-groopi-app/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "nx-electron:build",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/apps/electron-groopi-app",
            "main": "apps/electron-groopi-app/src/main.ts",
            "tsConfig": "apps/electron-groopi-app/tsconfig.app.json",
            "assets": [
              "apps/electron-groopi-app/src/assets"
            ]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/electron-groopi-app/src/environments/environment.ts",
                  "with": "apps/electron-groopi-app/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "nx-electron:execute",
          "options": {
            "buildTarget": "electron-groopi-app:build"
          }
        },
        "package": {
          "executor": "nx-electron:package",
          "options": {
            "name": "electron-groopi-app",
            "frontendProject": "react-groopi-app",
            "outputPath": "dist/packages",
            "prepackageOnly": true
          }
        },
        "make": {
          "executor": "nx-electron:make",
          "options": {
            "name": "electron-groopi-app",
            "frontendProject": "react-groopi-app",
            "outputPath": "dist/executables"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": [
            "{options.outputFile}"
          ],
          "options": {
            "lintFilePatterns": [
              "apps/electron-groopi-app/**/*.ts"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/electron-groopi-app"
          ],
          "options": {
            "jestConfig": "apps/electron-groopi-app/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "react-groopi-app": {
      "root": "apps/react-groopi-app",
      "sourceRoot": "apps/react-groopi-app/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/web:build",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/apps/react-groopi-app",
            "index": "apps/react-groopi-app/src/index.html",
            "baseHref": "/",
            "main": "apps/react-groopi-app/src/main.tsx",
            "polyfills": "apps/react-groopi-app/src/polyfills.ts",
            "tsConfig": "apps/react-groopi-app/tsconfig.app.json",
            "assets": [
              "apps/react-groopi-app/src/favicon.ico",
              "apps/react-groopi-app/src/assets"
            ],
            "styles": [],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/react-groopi-app/src/environments/environment.ts",
                  "with": "apps/react-groopi-app/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "react-groopi-app:build",
            "hmr": true,
            "proxyConfig": "apps/react-groopi-app/proxy.conf.json"
          },
          "configurations": {
            "production": {
              "buildTarget": "react-groopi-app:build:production",
              "hmr": false
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": [
            "{options.outputFile}"
          ],
          "options": {
            "lintFilePatterns": [
              "apps/react-groopi-app/**/*.{ts,tsx,js,jsx}"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/react-groopi-app"
          ],
          "options": {
            "jestConfig": "apps/react-groopi-app/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "react-groopi-app-e2e": {
      "root": "apps/react-groopi-app-e2e",
      "sourceRoot": "apps/react-groopi-app-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/react-groopi-app-e2e/cypress.json",
            "devServerTarget": "react-groopi-app:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "react-groopi-app:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": [
            "{options.outputFile}"
          ],
          "options": {
            "lintFilePatterns": [
              "apps/react-groopi-app-e2e/**/*.{js,ts}"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/react"
  },
  "generators": {
    "@nrwl/react": {
      "application": {
        "style": "styled-components",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "styled-components"
      },
      "library": {
        "style": "styled-components",
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "react-groopi-app"
}
